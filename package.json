{
  "name": "chordsync",
  "version": "1.0.0",
  "description": "This project is bootstrapped with [Create Elm App](https://github.com/halfzebra/create-elm-app).",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles --include-path ./node_modules src/styles/ -o src/styles/ --watch --recursive",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently --kill-others \"npm run watch-css\" \"elm-app start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinodronold/chordsync.git"
  },
  "author": "Vinod Ronold",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/vinodronold/chordsync/issues"
  },
  "homepage": "https://github.com/vinodronold/chordsync#readme",
  "dependencies": {
    "material-components-web": "^0.38.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "node-sass-chokidar": "^1.3.3"
  }
}
